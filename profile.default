# ~/.profile.local
# Created by Spencer Bliven
#
# This contains default .profile settings which work for many machines

# SET UP PATHS
# Lower entries are searched first

# home directory
[[ -d ~/bin ]] && PATH="~/bin:$PATH"
[[ -d ~/man ]] && MANPATH="~/man:$MANPATH"

# current directory
PATH=".:bin:$PATH"
MANPATH="man:$MANPATH"
export PATH MANPATH

